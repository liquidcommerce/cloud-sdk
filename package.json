{
 "name": "@liquidcommerceteam/cloud-sdk",
 "version": "1.9.0-beta.5",
 "description": "LiquidCommerce Cloud SDK",
 "main": "./dist/index.cjs",
 "module": "./dist/index.esm.js",
 "browser": "./umd/liquidcommerce-cloud-sdk.js",
 "types": "./dist/types/index.d.ts",
 "packageManager": "pnpm@9.5.0",
 "exports": {
  ".": {
   "import": {
    "types": "./dist/types/index.d.ts",
    "default": "./dist/index.esm.js"
   },
   "require": {
    "types": "./dist/types/index.d.ts",
    "default": "./dist/index.cjs"
   },
   "node": {
    "types": "./dist/types/index.d.ts",
    "default": "./dist/liquidcommerce-cloud-sdk.ssr.js"
   },
   "browser": {
    "types": "./dist/types/index.d.ts",
    "default": "./umd/liquidcommerce-cloud-sdk.js"
   }
  }
 },
 "files": [
  "dist",
  "umd"
 ],
 "type": "module",
 "sideEffects": false,
 "scripts": {
  "build": "rollup -c --environment NODE_ENV:production",
  "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
  "dev": "rollup -c --environment NODE_ENV:development -w",
  "test": "vitest run",
  "test:watch": "vitest",
  "test:coverage": "vitest run --coverage",
  "lint": "eslint \"src/**/*.ts\"",
  "format": "prettier --write \"src/**/*.ts\"",
  "docs": "typedoc --out docs src",
  "prepublishOnly": "pnpm run build"
 },
 "keywords": [
  "liquidcommerce",
  "ecommerce",
  "alcohol",
  "beverage",
  "cloud-sdk"
 ],
 "author": "ReserveBar Tech",
 "license": "",
 "devDependencies": {
  "@eslint/eslintrc": "^3.1.0",
  "@eslint/js": "^9.9.1",
  "@rollup/plugin-commonjs": "^26.0.1",
  "@rollup/plugin-node-resolve": "^15.2.3",
  "@rollup/plugin-replace": "^5.0.7",
  "@semantic-release/changelog": "^6.0.3",
  "@semantic-release/git": "^10.0.1",
  "@semantic-release/github": "^10.1.7",
  "@semantic-release/npm": "^12.0.1",
  "@types/eslint__js": "^8.42.3",
  "@types/node": "^20.0.0",
  "@typescript-eslint/eslint-plugin": "^8.2.0",
  "@typescript-eslint/parser": "^8.2.0",
  "@vitest/coverage-v8": "^1.0.0",
  "conventional-changelog-cli": "^5.0.0",
  "dotenv": "^16.4.5",
  "eslint": "^9.9.1",
  "eslint-config-prettier": "^9.1.0",
  "eslint-import-resolver-typescript": "^3.6.1",
  "eslint-plugin-import": "^2.29.1",
  "eslint-plugin-simple-import-sort": "^12.1.1",
  "eslint-plugin-sonarjs": "^2.0.1",
  "eslint-plugin-unicorn": "^55.0.0",
  "husky": "^9.1.5",
  "jest": "^29.7.0",
  "prettier": "^3.3.3",
  "rollup": "^4.21.0",
  "rollup-plugin-livereload": "^2.0.5",
  "rollup-plugin-serve": "^1.1.1",
  "rollup-plugin-terser": "^7.0.2",
  "rollup-plugin-typescript2": "^0.36.0",
  "semantic-release": "^24.1.0",
  "ts-jest": "^29.2.5",
  "typedoc": "^0.26.6",
  "typescript": "^5.5.4",
  "typescript-eslint": "^8.2.0",
  "vite": "^5.0.0",
  "vitest": "^1.0.0"
 },
 "engines": {
  "node": ">=20"
 },
 "pnpm": {
  "peerDependencyRules": {
   "ignoreMissing": []
  }
 },
 "dependencies": {
  "@stripe/stripe-js": "^7.3.1"
 }
}
